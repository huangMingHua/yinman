<!--pages/student/myCourse/selectClass/selectClass.wxml-->
<view>
  <view class='term'>
    <image src='data:image/png;base64,'></image>
    <text>学期起止</text>
    <text>{{term.startDate}}~{{term.endDate}}</text>
  </view>
  <view class='courseDetail'>
    <image src='{{user.wxHead}}'></image>
    <view>
      <view><text>教师</text><text>{{teacher.name}}</text></view>
      <view class='class'>
        <text>{{course.name}}</text>
        <text>{{courseTableItem.duration}}分钟</text>
        <text>{{courseTableItem.number}}人</text>
        <text>等级{{levels[courseTableItem.level-1].name}}</text>
      </view>
    </view>
  </view>
  <view class='line'>
  </view>
  <view class="switchWeek">
    <view bindtap='lastWeek' class="{{index===1?'gray':'blue'}}">
      <image wx:if="{{index===1}}" class="gray" src='data:image/png;base64,'></image>
      <image class="blue" wx:if="{{index>1}}" src='data:image/png;base64,'></image>
      <text>上一周</text>
    </view>
    <view class='weekDate'>
      <text>第{{weekIndex}}周</text>
      <text>{{wStartDate}}-{{wEndDate}}</text>
    </view>
    <view bindtap='nextWeek' class="{{index===weekNum?'gray':'blue'}}">
      <text>下一周</text>
      <image class="gray" wx:if="{{index===weekNum}}" src='data:image/png;base64,'></image>
      <image class="blue" wx:if="{{index<weekNum}}" src='data:image/png;base64,'></image>
    </view>
  </view>
  <view class='classTable'>
    <view class="row">
      <view><text>星期</text></view>
      <view class='weekTh'><text>时间</text></view>
      <view class='weekTh'><text>已报/满员</text></view>
    </view>
    <view class="row">
      <view><text>星期一</text></view>
      <view>
        <view bindtap='selectCalss' data-id="{{item.id}}" class="{{item.index===0?'weekCon':'weekCon1'}}" wx:for="{{classTableInfo[0].times}}">
          <text>{{item.startTime}}-{{item.endTime}}</text>
          <image wx-if="{{item.index===2}}"  src='data:image/png;base64,'></image>
        </view>
      </view>
      <view>
        <view class="weekCon" wx:for="{{classTableInfo[0].number}}">
          <text>{{item.signUpNum}}/{{item.totalNum}}</text>
        </view>
      </view>
    </view>
    <view class="row">
      <view><text>星期二</text></view>
      <view>
        <view bindtap='selectCalss' data-id="{{item.id}}" class="{{item.index===0?'weekCon':'weekCon1'}}" wx:for="{{classTableInfo[1].times}}">
          <text>{{item.startTime}}-{{item.endTime}}</text>
          <image wx-if="{{item.index===2}}" src='data:image/png;base64,'></image>
        </view></view>
      <view>
        <view class="weekCon" wx:for="{{classTableInfo[1].number}}">
          <text>{{item.signUpNum}}/{{item.totalNum}}</text>
        </view>
      </view>
    </view>
    <view class="row">
      <view><text>星期三</text></view>
      <view>
        <view bindtap='selectCalss' data-id="{{item.id}}" class="{{item.index===0?'weekCon':'weekCon1'}}" wx:for="{{classTableInfo[2].times}}">
          <text>{{item.startTime}}-{{item.endTime}}</text>
          <image wx-if="{{item.index===2}}"  src='data:image/png;base64,'></image>
        </view>
      </view>
      <view>
        <view class="weekCon" wx:for="{{classTableInfo[2].number}}">
          <text>{{item.signUpNum}}/{{item.totalNum}}</text>
        </view>
      </view>
    </view>
    <view class="row">
      <view><text>星期四</text></view>
      <view>
        <view bindtap='selectCalss' data-id="{{item.id}}" class="{{item.index===0?'weekCon':'weekCon1'}}" wx:for="{{classTableInfo[3].times}}">
          <text>{{item.startTime}}-{{item.endTime}}</text>
          <image wx-if="{{item.index===2}}"  src='data:image/png;base64,'></image>
        </view>
      </view>
      <view>
        <view class="weekCon" wx:for="{{classTableInfo[3].number}}">
          <text>{{item.signUpNum}}/{{item.totalNum}}</text>
        </view>
      </view>
    </view>
    <view class="row">
      <view><text>星期五</text></view>
      <view>
        <view bindtap='selectCalss' data-id="{{item.id}}" class="{{item.index===0?'weekCon':'weekCon1'}}" wx:for="{{classTableInfo[4].times}}">
          <text>{{item.startTime}}-{{item.endTime}}</text>
           <image wx-if="{{item.index===2}}"  src='data:image/png;base64,'></image>
        </view>
      </view>
      <view>
        <view class="weekCon" wx:for="{{classTableInfo[4].number}}">
          <text>{{item.signUpNum}}/{{item.totalNum}}</text>
        </view>
      </view>
    </view>
    <view class="row">
      <view><text>星期六</text></view>
      <view>
        <view bindtap='selectCalss' data-id="{{item.id}}" class="{{item.index===0?'weekCon':'weekCon1'}}" wx:for="{{classTableInfo[5].times}}">
          <text>{{item.startTime}}-{{item.endTime}}</text>
           <image wx-if="{{item.index===2}}"  src='data:image/png;base64,'></image>
        </view>
      </view>
      <view>
        <view class="weekCon" wx:for="{{classTableInfo[5].number}}">
          <text>{{item.signUpNum}}/{{item.totalNum}}</text>
        </view>
      </view>
    </view>
    <view class="row">
      <view><text>星期日</text></view>
      <view>
        <view bindtap='selectCalss' data-id="{{item.id}}" class="{{item.index===0?'weekCon':'weekCon1'}}" wx:for="{{classTableInfo[6].times}}">
          <text>{{item.startTime}}-{{item.endTime}}</text>
          <image wx-if="{{item.index===2}}"  src='data:image/png;base64,'></image>
        </view>
      </view>
      <view>
         <view class="weekCon" wx:for="{{classTableInfo[6].number}}">
          <text>{{item.signUpNum}}/{{item.totalNum}}</text>
        </view>
      </view>
    </view>
  </view>
  <view class='submit'>
    <button bindtap='submit'>确认选课</button>
  </view>
</view>
