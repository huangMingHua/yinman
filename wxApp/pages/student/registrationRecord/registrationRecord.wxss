/* pages/student/registrationRecord/registrationRecord.wxss */
scroll-view{
  padding-bottom:104rpx; 
}
.header{
  display: flex;
  align-items: center;
  background-color: #f5f5f5;
  height: 84rpx;
  font-size: 32rpx;
  color: #040000;
  border-bottom: 1rpx solid #e6e6e6;
}
.header image{
  margin: 0 15rpx 0 32rpx;
  width: 38rpx;
  height: 38rpx;
}
.list{
  display: flex;
  padding: 25rpx 0;
  font-size: 28rpx;
}
.detail{
  line-height: 37rpx;
}
.list .identification{
  width: 28rpx;
  height: 28rpx;
  margin: 0 20rpx 0 38rpx;
}
.list .state{
  display: flex;
  flex: 1;
  justify-content: flex-end;
  align-items: center;
  font-size: 24rpx;
  color: #ff8004;
}
.list .dir{
  width: 20rpx;
  height: 34rpx;
  margin: 0 30rpx 0 10rpx;
}
.btn{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 104rpx;
  background-color: #3cb4ff;
  font-size: 28rpx;
  color: white;
  padding-left: 0;
  padding-right: 0;
  margin-left: 0;
  margin-right: 0;
  line-height: 104rpx;
  text-align: center;
  border-radius: 0;
}