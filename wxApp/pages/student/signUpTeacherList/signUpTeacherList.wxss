/* pages/student/signUpTeacherList/signUpTeacherList.wxss */
page{
  background-color: #f5f5f5;
}
.terms{
  background-color: white;
  padding: 32rpx 34rpx;
  border-bottom: 1rpx solid #e6e6e6;
}
.pickerTerm{
  width: 100%;
  background-color: #ebebeb;
  height: 80rpx;
  display: flex;
  align-items: center;
  border-radius: 5px;
  position: relative; 
}
.pickerTermTitle{
  font-size: 32rpx;
  color: #b9b9b9;
  margin: 0 26rpx; 
}
.termSelect{
  font-size: 28rpx;
  color: #333333;
}
.down {
  position: absolute;
  right: 36rpx;
  top: 28rpx; 
  width: 40rpx;
  height: 24rpx;
}
.list{
  position: relative;
  margin-top: 19rpx;
  padding: 36rpx 32rpx;
  background-color: white;
}
.teacherTitle{
  margin: 0 20rpx;
  font-size: 32rpx;
  color: #040000;
}
.teacher{
  font-size: 32rpx;
  color: #040000;
}
.btn{
  position: absolute;
  right: 32rpx;
  top: 40rpx;
  width: 120rpx;
  height: 60rpx;
  background-color: #3cb4ff;
  font-size: 28rpx;
  color: white;
  display: inline-block;
  padding-left: 0;
  padding-right: 0;
  margin-left: 0;
  margin-right: 0;
  line-height: 60rpx;
  text-align: center;
  border-radius: 5px;
}
.personalProfile{
  margin-top: 16rpx;
  margin-bottom: 18rpx;
  font-size: 28rpx;
  color: #979797;
  line-height: 46rpx;
  width: 686rpx;
  word-wrap:break-word;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}
.headPortrait{
  width: 70rpx;
  height: 70rpx;
  vertical-align: middle;
  border-radius: 100%;
}
.type{
  width: 28rpx;
  height: 28rpx;
  vertical-align: middle;
  margin-left: 16rpx;
  margin-right: 10rpx;
}
.classes{
  font-size: 28rpx;
}
.classes view{
  width: 50%;
  height: 68rpx;
  line-height: 68rpx;
  display: inline-block;
  background-color: #f5f5f5;
  margin-bottom: 2rpx;
}
.classes view text{
  width: 80%;
  height: 68rpx;
  line-height: 68rpx;
  vertical-align: middle;
  display:-webkit-inline-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
  overflow:hidden;
}
.noData{
  padding-top: 112rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.noData image{
  margin-bottom: 30rpx;
  width: 192rpx;
  height: 220rpx;
}
.noData text{
  color: #89888f;
  font-size: 36rpx;
}