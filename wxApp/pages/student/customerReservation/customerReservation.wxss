/* pages/student/customerReservation/customerReservation.wxss */
page{
  background-color: #f5f5f5;
}
.student{
   height: 100rpx;
   line-height: 100rpx;
   font-size: 32rpx;
   border-bottom: 1rpx solid #e6e6e6;
   background-color: white;
}
.student text:nth-child(1){
  margin: 0 5rpx 0 30rpx; 
}
.courseTableDetailList{
  margin-top: 20rpx;
  padding-top: 20rpx;
  border-top: 1rpx solid #e6e6e6;
  border-bottom: 1rpx solid #e6e6e6;
  background-color: white;
}
.courseTableDetailList .teacher{
   display: flex;
   height: 98rpx;
   align-items: center;
   font-size: 40rpx;
}
.courseTableDetailList .teacher image{
  margin: 0 20rpx 0 30rpx; 
  width: 70rpx;
  height: 70rpx;
  border-radius: 100%;
}
.courseTableDetailList .teacher text:nth-of-type(1){
   margin-right: 18rpx;
} 
.courseTableDetailList .courseIntroduce{
  padding: 0 35rpx 30rpx 35rpx;
  word-break: break-all;
  font-size: 28rpx;
  color: #979797; 
}
.courseTableDetailList .courseDetails{
  position: relative;
  display: flex;
  margin: 0 30rpx 36rpx 30rpx;
  padding: 20rpx 0; 
  background-color: #f5f5f5;
}
.courseTableDetailList .courseDetails>view:nth-child(1){
  margin: 0 10rpx 0 20rpx; 
}
.courseTableDetailList .courseDetails>view:nth-child(2){
  font-size: 28rpx;
  line-height: 50rpx;
}
.courseTableDetailList .courseDetails image{
  width: 28rpx;
  height: 28rpx;
}
.courseTableDetailList .courseDetails navigator{
  position: absolute;
  right: 12rpx;
  top: 38rpx;
  width: 112rpx;
  height: 64rpx;
  font-size: 32rpx;
  text-align: center;
  line-height: 64rpx;
  border-radius: 5px;
  color: white;
  background-color: #3cb4ff;
} 
.noData{
  padding-top: 112rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.noData image{
  margin-bottom: 30rpx;
  width: 192rpx;
  height: 220rpx;
}
.noData text{
  color: #89888f;
  font-size: 36rpx;
}