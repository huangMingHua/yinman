.btn{
    padding: 40rpx 0;
    position: relative;
}
.btn button{

    width: 278rpx;
    height: 90rpx;
    text-align: center;
    line-height: 90rpx;
    border-radius: 5px;
    color: white;
    background-color:#3cb4ff; 
}
.tel{
    background-color: white;
    padding: 30rpx 0 1rpx 0;
    margin-top: 20rpx;
}
.reminder{
  margin-top: 20rpx;
  font-size: 24rpx;
  background-color: white;
  padding:0 32rpx;
  color:#979797;
  line-height:40rpx;
  padding-top: 20rpx;
}
.reminder view:nth-child(1){
  height:46rpx;
  line-height:46rpx;
  font-size:28rpx;
  color:#040000;
}
.reminder view:nth-child(2),.reminder view:nth-child(3){
  padding-left: 40rpx;
}
.reminder image{
  width:34rpx;
  height: 34rpx;
  vertical-align: middle;
}
