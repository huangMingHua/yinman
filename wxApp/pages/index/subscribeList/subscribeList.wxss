/* pages/index/subscribeList/subscribeList.wxss */
@import "../../template/prompt/prompt.wxss";
@import "../../template/contact/contact.wxss";
page{
    background-color: #f5f5f5;
}
.list{
    position: relative;
    background-color: white;
    margin-bottom: 20rpx;
}
.list .head{
    line-height: 100rpx;
    height: 100rpx;
}
.list .head>text{
    font-size: 32rpx;
    vertical-align: middle;
    margin-left: 32rpx;
}
.list .textWrap{
    float: right;
}
.list .head text{
    font-size: 24rpx;
    vertical-align: middle;
}
.list .head image{
    width: 14rpx;
    height: 24rpx; 
    vertical-align: middle;
    margin: 0 30rpx 0 15rpx;
}
.list .timeTable{
    height: 60rpx;
    line-height: 60rpx;
    border-bottom: 2rpx  solid #f4f4f4;
}
.list .timeTable image{
    width: 30rpx;
    height: 30rpx;
    margin:0 16rpx 0 32rpx;
    vertical-align: middle;
}
.list .timeTable text{
    vertical-align: middle;
    color: #979797;
    font-size: 28rpx;
}
