/* pages/student/studentList/studentList.wxss */
Page{
  background-color:#f5f5f5; 
}
.list{
  position: relative;
  height: 100rpx;
  line-height: 100rpx;
  background-color: white;
  margin-bottom: 20rpx;
  font-size: 30rpx;
}
.list .name{
  display: inline-block;
  width:500rpx;
  padding-left: 20rpx;
}
.add{
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  margin-top: 20rpx;
  background-color: white;
  color: #14a2d4;
  font-size: 30rpx;
}
.order,.edit,.delete{
  position: absolute;
  width: 100rpx;
  color: #14a2d4;
  font-size: 30rpx;
  text-align: center;
}
.order{
  right:200rpx;
  top: 0;

}
.edit{
  right:100rpx;
  top: 0;
}
.delete{
  right:0;
  top: 0;
}