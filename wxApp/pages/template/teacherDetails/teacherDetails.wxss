.courseDetails {
  display: flex;
  flex-direction: column;
  background-color: white;
  padding: 0 32rpx;
}

.courseDetailsH {
  display: flex;
  flex-direction: row;
  height: 117rpx;
  font-size: 32rpx;
}

.courseDetailsH>view {
  height: 100%;
  line-height: 70rpx;
  padding: 28rpx 0 20rpx 0;
  box-sizing: border-box;
}

.courseDetailsH>view:nth-child(1) {
  margin-right: 18rpx;
}

.courseDetailsH image {
  width: 70rpx;
  height: 70rpx;
  border-radius: 100%;
}

.courseDetailsH>view:nth-child(2) view {
  height: 28rpx;
  line-height: 28rpx;
}

.courseDetailsH>view:nth-child(2) view:nth-child(1) {
  margin-bottom: 18rpx;
}

.courseDetailsH .classInfo {
  color: #979797;
}

.timeInfo {
  padding: 6rpx 30rpx 28rpx 30rpx;
  background-color: #f5f5f5;
}

.timeInfo view {
  margin-top: 22rpx;
  font-size: 28rpx;
}

.btn-area-a {
  display: flex;
  flex-direction: row;
  padding: 18rpx 0 30rpx 0;
  font-size: 28rpx;
}

.btn-area-a navigator {
  width: 220rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 5rpx;
  text-align: center;
  color: white;
}

.btn-area-a .btn-area-A {
  background-color: #01d6c2 !important;
  margin: 0 16rpx 0 232rpx;
}

.btn-area-a .btn-area-B {
  background-color: #ff9b39 !important;
}

.curriculum-schedule {
  background-color: white;
  border-left: none;
  border-right: none;
}

.curriculum-schedule .curriculum-schedule-head {
  padding: 5px;
  height: 106rpx;
}

.curriculum-schedule .curriculum-schedule-head .curriculum-schedule-week {
  text-align: center;
  width: 185rpx;
  height: 106rpx;
  font-size: 28rpx;
  display: inline-block;
}

.curriculum-schedule .curriculum-schedule-head .week {
  margin: 26rpx 0 10rpx 0;
}

.curriculum-schedule .curriculum-schedule-head .active {
  background-color: #3cb4ff;
  border-radius: 5px;
  color: white;
}

.cul-week {
  color: #979797;
  margin-right: 20rpx;
  font-size: 28rpx;
}

.table-head {
  display: flex;
  flex-direction: row;
  border: 1rpx solid #e6e6e6;
  border-left: none;
  border-right: none;
}

.table-head>view {
  height: 64rpx;
  line-height: 64rpx;
}

.table-head .head-week, .table-body .body-week {
  width: 142rpx;
  text-align: center;
  font-size: 28rpx;
  color: #333;
}

.table-head .head-cul, .table-head .head-time, .table-head .head-do {
  width: 200rpx;
  text-align: center;
  border-left: 1rpx solid #f4f4f4;
  font-size: 28rpx;
  color: #333;
}

.table-body .row {
  display: flex;
  flex-direction: row;
  min-height: 100rpx;
}

.table-body .body-week {
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1rpx solid #e6e6e6;
  border-left: none;
  border-top: none;
}

.table-body .body-cul, .table-body .body-time {
  padding: 6rpx;
  padding-bottom: 0;
  font-size: 24rpx;
  width: 188rpx;
  border: 1rpx solid #e6e6e6;
  border-left: none;
  border-top: none;
}

.table-body .cul-contenct {
  margin-bottom: 6rpx;
  background-color: #edf6fe;
  width: 188rpx;
  height: 100rpx;
  border-radius: 5px;
  text-align: center;
}

.table-body .body-do {
  width: 188rpx;
  text-align: center;
  line-height: 100rpx;
  color: #25abff;
  font-size: 24rpx;
  border: 1rpx solid #e6e6e6;
  border-left: none;
  border-top: none;
}
