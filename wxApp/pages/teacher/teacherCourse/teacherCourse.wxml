<!--pages/teacher/teacherCourse/teacherCourse.wxml-->
 <picker  bindchange="semesterChange" class="semester" value="{{semesterIndex}}" range="{{semesterArr}}">
    <view class="semesterPicker">
      {{semesterArr[semesterIndex]}}<image class="img" wx:if="{{semesterArr[semesterIndex]}}" src="/images/icon/blunDown.png"></image>
      <text wx:else>{{loading}}</text>
    </view>
    <view class="semesterStartEnd">
      <text>学期起止：{{DateSatartEnd}}</text>
    </view>
</picker>
<view class="teahcerDetail">
  <image src='{{user.wxHead}}'></image>
  <text class='teacherTitle'>教师</text>
  <text class='teacherName'>{{teacherName}}</text>
</view>
<view class="switchWeek">
    <view bindtap='lastWeek' class="{{index===1?'gray':'blue'}}">
      <image wx:if="{{index===1}}" class="gray" src='data:image/png;base64,'></image>
      <image class="blue" wx:if="{{index>1}}" src='data:image/png;base64,'></image>
      <text>上一周</text>
    </view>
    <view class='weekDate'>
      <text>第{{weekIndex}}周</text>
      <text>{{wStartDate}}-{{wEndDate}}</text>
    </view>
    <view bindtap='nextWeek' class="{{index===weekNum?'gray':'blue'}}">
      <text>下一周</text>
      <image class="gray" wx:if="{{index===weekNum}}" src='data:image/png;base64,'></image>
      <image class="blue" wx:if="{{index<weekNum}}" src='data:image/png;base64,'></image>
    </view>
  </view>
