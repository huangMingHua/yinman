<!--pages/teacher/checkWork/checkWork.wxml-->
<view class='teahcerDetailWrap'>
  <view class="teahcerDetail">
    <image src="{{studentUser.wxHead}}"></image>
    <view class='student'>
      <view>
        <text>{{student.name}}</text>
      </view>
      <view>
        <text>{{course.name}} {{courseTableItem.duration}}分钟</text>
      </view>
    </view>
  </view>
  <view class='classDetail'>
    <view><text>课程：</text><text>{{course.name}}</text></view>
    <view><text>时间：</text><text>{{courseTableItem.date}} 星期{{cnum[courseTableItem.dayOfWeek]}} {{courseTableItem.startTime}}~{{courseTableItem.endTime}}</text></view>
     <view><text>满员人数：</text><text>{{courseTableItem.number}}</text></view>
    <view><text>等级：</text><text>{{levels[courseTableItem.level-1].name}}</text></view>
    <view><text>教室：</text><text>{{classroom.name}}</text></view> 
  </view>
</view>
<view class='title'>
  <image src='data:image/png;base64,'></image> <text>教师评论</text>
</view>
<view class='commentListWrap'>
<view class='commentList' wx:for="{{commentList}}">
  <view class='head'><image src="{{teacherUser.wxHead}}"></image></view>
  <view class='comment'>
    <view><text>教师：{{teacher.name}}</text></view>
    <view><text>{{item.comment}}</text></view>
    <view><text>{{item.createTime}}</text></view>
  </view>
</view>
</view>
<view class='btn'>
  <view>
    <view class='input'><image src='data:image/png;base64,'></image>
    <input bindinput="bindInputComment" placeholder="我来点评一下~" value="{{commentValue}}" />
   </view>
   <button bindtap='addMoment'>确认</button>
  </view>
</view>

