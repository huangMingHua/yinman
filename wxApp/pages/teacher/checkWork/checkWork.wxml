<!--pages/teacher/checkWork/checkWork.wxml-->
<view class='teahcerDetailWrap'>
  <view class="teahcerDetail">
    <image src='{{teacherUser.wxHead}}'></image>
    <text class='teacherTitle'>教师</text>
    <text class='teacherName'>{{teacher.name}}</text>
  </view>
  <view class='classDetail'>
    <view><text>课程：</text><text>{{courseName.name}}</text></view>
    <view><text>时间：</text><text>{{courseTableItem.date}} 星期{{ cnum[courseTableItem.dayOfWeek]}} {{courseTableItem.startTime}}~{{courseTableItem.endTime}}</text></view>
     <view><text>满员人数：</text><text>{{courseTableItem.number}}</text></view>
    <view><text>等级：</text><text>{{levels[courseTableItem.level-1].name}}</text></view>
    <view><text>教室：</text><text>{{classroom.name}}</text></view> 
  </view>
</view>
<view class='title'>
  <text>我的学生</text>
</view>
<view class='table'>
  <view class='row' wx:for="{{courseTableItemStudents}}">
    <view class='selected' bindtap='select' data-id='{{item.id}}' >
    <image   wx:if="{{courseTableItem.isChecked===0&&item.isGoClass===1}}"  src='data:image/png;base64,'></image>
    <image wx:if="{{courseTableItem.isChecked===0&&item.isGoClass===0}}" src='data:image/png;base64,'></image></view> 
    <view class='headPortraitWrap'>
      <image class='headPortrait' src='{{item.user.wxHead}}'></image>
      <image class='isGoClss' wx:if="{{courseTableItem.isChecked===1&&item.isGoClass===1}}" src='data:image/png;base64,'></image>
      <image wx:if="{{courseTableItem.isChecked===1&&item.isGoClass===0}}" class='isGoClss' src='data:image/png;base64,'></image>
    </view>
    <text>{{item.student.name}}</text>
    <navigator url='/pages/teacher/teacherComment/teacherComment?studentId={{item.studentId}}&courseTableItemId={{item.courseTableItemId}}'>点评学生</navigator>
    <navigator>请假和补课记录</navigator>
  </view>
  <view class='btn' wx:if="{{courseTableItemStudents.length>0&&courseTableItem.isChecked==0}}">
    <button bindtap="submit">确认考勤</button>
  </view>
</view>
