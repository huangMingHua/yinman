/* pages/yinmanIntroduce/yinmanIntroduce.wxss */
.wrap{
    padding: 32rpx 36rpx;
}
.head>view:nth-child(1) text{
    font-size: 48rpx;
    color: #040000;
    vertical-align: top;
} 
.head>view:nth-child(1) view{
  float: right;
}
.head>view:nth-child(1) view image{
    width: 39rpx;
    height: 37rpx;
    vertical-align: middle;
    margin-right: 10rpx;
}
.head>view:nth-child(1) view text{
    width: 39rpx;
    height: 37rpx;
    font-size: 24rpx;
    color: #b9b9b9;
    vertical-align: middle;
}
.head>view:nth-child(2){
    font-size: 24rpx;
    color: #b9b9b9;
    height: 82rpx;
    line-height: 82rpx;
    margin-bottom: 28rpx;
}
.head>view:nth-child(2) text:nth-child(1){
    margin-right: 22rpx;
}
.music{
    width: 100%;
    height: 463rpx;
}
.p{
    margin-top: 24rpx;
    font-size: 32rpx;
    line-height: 48rpx;
}