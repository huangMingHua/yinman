.classEnroll {
  display: flex;
  flex-direction: column;
}

.class, .enroll {
  height: 110rpx;
  line-height: 110rpx;
  background-color: #fcfcfc;
  border-bottom: 1rpx solid #e6e6e6;
}

.class image, .enroll image {
  margin-left: 32rpx;
  width: 50rpx;
  height: 50rpx;
  vertical-align: middle;
}

.class text, .enroll text {
  margin-left: 22rpx;
  color: #333;
  vertical-align: middle;
  font-size: 32rpx;
}

.class .dir, .enroll .dir {
  width: 12rpx;
  height: 27rpx;
  float: right;
  margin-right: 30rpx;
  margin-top: 41rpx;
}
